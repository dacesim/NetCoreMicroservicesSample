{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Ocelot": "Warning"
    }
  },

  "AllowedHosts": "*",

  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration",
    "UseServiceDiscovery": true,
    "ServiceDiscoveryProvider": {
      "Type": "Consul",
      "Host": "localhost",
      "Port": 8500
    }
  },
  "RateLimitOptions": {
    "ClientWhitelist": [],
    "EnableRateLimiting": true,
    "Period": "1s",
    "PeriodTimespan": 1,
    "Limit": 1
  },

  "HttpHandlerOptions": {
    "UseTracing": true
  }
}
